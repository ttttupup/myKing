<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <TextView
        android:id="@+id/testID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="hello"
        android:textSize="32sp"/>
    <TextView
        android:id="@+id/testJump"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="jump"
        android:textSize="32sp"/>
    <TextView
        android:id="@+id/testJumpBan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="banner"
        android:textSize="32sp"/>
    <TextView
        android:id="@+id/testJumpA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="auth"
        android:textSize="32sp"/>
    <TextView
        android:id="@+id/tv_webtest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="web"
        android:textSize="32sp"/>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/selector_grid_menu"/>

    </LinearLayout>
    <ProgressBar
        android:id="@+id/pb_test_1"
        style = "?android:attr/progressBarStyleHorizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>
